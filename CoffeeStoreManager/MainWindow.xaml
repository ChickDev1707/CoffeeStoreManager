<Window x:Class="CoffeeStoreManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoffeeStoreManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1150">
    <Grid>
        <StackPanel VerticalAlignment="Center" Width="200" Height="200">
            <Button Content="Manage Food" Margin="0 0 0 10" Width="100" AutomationProperties.Name="MangeFoodBtn" Click="OpenManageFoodWindow"></Button>
            <Button x:Name="ManageSourceBtn" Content="Manage Source" Width="100" AutomationProperties.Name="MangeFoodBtn" Click="OpenManageSourceWindow"></Button>
        </StackPanel>
    </Grid>
</Window>
